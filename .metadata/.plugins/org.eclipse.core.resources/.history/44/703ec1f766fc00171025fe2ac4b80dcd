package textproc;
import java.util.*;
import java.util.Map.Entry;

public class WordCountComparator implements Comparator<Map.Entry<String,Integer>>{

	public int compare(Entry<String, Integer> o1, Entry<String, Integer> o2) {
		int e1 = o1.getValue();
		int e2 = o2.getValue();
		if(e1>e2){
			return e1-e2;
		}
		else if(e1<e2){
			return e1-e2;
		}
		
	}

	
	
	

}
