package mountain;

public class Side {
	private Point p1;
	private Point p2;
	
	
	
	public Side(Point p1, Point P2){
		this.p1 = p1;
		this.p2 = p2;
	}
	
	public boolean equals(Object obj){
		if (obj instanceof Side){
			Side s = (Side) obj;
			return s.p1 == obj.p1 && s.p2 == obj.p2;
		}
		
	}
	
	public int hashCode(){
		return p1.hashCode() +p2.hashCode();
	}
	
}
